# deviceId = -1 for CPU, >= 0 for GPU devices
deviceId = -1

# note we're only using 2 inputs here, and we're using b because it's smaller
SparseSeqPart = [
    precision = "float"
    reader = [
        readerType = "CNTKBinaryReader"
        file = "sparseseqoutput.bin"

        input = [
            features = [ original="b" ]
            labels = [ original="d" ]
        ]
        randomize = false
    ]
]

